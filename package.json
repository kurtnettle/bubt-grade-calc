{
  "name": "bubt-grade-calc",
  "version": "2.0",
  "description": "Calculate your academic grade at ease. (attempt of filling the void which Annex failed)",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "format": "prettier . --write",
    "lint": "xo src/",
    "lint:fix": "xo --fix src/",
    "watch": "rollup -cw"
  },
  "keywords": [
    "bubt",
    "annex",
    "academic",
    "result"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/chrome": "^0.1.0",
    "@types/firefox-webext-browser": "^120.0.4",
    "rollup": "^4.45.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "web-ext": "^8.8.0",
    "xo": "^1.1.1"
  },
  "xo": {
    "space": true,
    "prettier": true,
    "env": [
      "browser",
      "node"
    ],
    "rules": {
      "n/file-extension-in-import": "off"
    }
  },
  "author": "Shaikh Shafeen <shafeen@duck.com> (https://github.com/kurtnettle)",
  "license": "GPL-3.0-only",
  "packageManager": "pnpm@10.13.1"
}
